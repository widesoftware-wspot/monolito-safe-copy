                });

                $('#birthdays').hide();

                var data = {
                    'field': ['data_nascimento', 'birthday', 'birthday_month']
                };

                var route = Routing.generate('check_field_exists');

                $.ajax({
                    type: "POST",
                    url: route,
                    data: data,
                    dataType : "json",
                    success: function(response)
                    {
                        if (response) {
                            $('#birthdays').show();
                        }
                    }
                });

                // Exibição dos alertas de PoC.
                function isNumber(n) {
                    return !isNaN(parseFloat(n)) && isFinite(n);
                }

                {% if app.session.get('wspotClient') is not null%}
                    {% if (app.session.get('wspotClient').status == constant('Wideti\\DomainBundle\\Entity\\Client::STATUS_POC'))
                        and ("now"|date('Y-m-d') <= app.session.get('wspotClient').pocEndDate|date('Y-m-d')) %}
                        var changePlanRoute = Routing.generate('admin_token_redirect');
                        var text = '<strong>Atenção!</strong> O seu período de testes se ';

                        {% if app.session.get('wspotClient').pocEndDate | DateDiffInDays == '0 dias' %}
                            text += ' encerra hoje.';
                        {% else %}
                            text += 'encerrará em {{ app.session.get('wspotClient').pocEndDate | DateDiffInDays }}.';
                        {% endif %}

                        var createdBy = "{{ app.session.get('wspotClient').createdBy }}";
                        if (createdBy === 'contratacao-automatica') {
                            text += '<a href="'+ changePlanRoute +'" class="purchase">Contratar</a>';
                        }

                        jQuery('#notification').notyfy({
                            text: text,
                            type: 'warning',
                            closeWith: ['button']
                        });
                    {% elseif (app.session.get('wspotClient').status == constant('Wideti\\DomainBundle\\Entity\\Client::STATUS_POC')
                        and "now"|date('Y-m-d') > app.session.get('wspotClient').pocEndDate|date('Y-m-d')) %}
                        bootbox.dialog("" +
                            "<h3>Olá, {{ app.user.nome }}</h3>" +
                            "Seu período de testes do Mambo WiFi chegou ao fim. Esperamos que você tenha encontrado na " +
                            "solução um excelente parceiro para gestão da sua rede e do relacionamento com clientes." +
