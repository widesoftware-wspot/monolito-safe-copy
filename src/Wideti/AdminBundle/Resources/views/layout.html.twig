    {% if app.session.get('wspotClient') != null %}
        {% if app.session.get('wspotClient').isWhiteLabel == true %}
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon2.ico') }}" />
        {% endif %}
    {% endif %}

    <style>

    .btn-mambo-position {
       top:  -5px;
    }

    .menu-mambo-position  {
        position: absolute;
        z-index: 10;
    }
    
    .menu-mambo-position .slimScrollDiv, .menu-mambo-position .slimScrollDiv .slim-scroll {
        height: 100% !important;
    }

    .contentRemoveMargin {
        margin: 0 !important;
    }

    .remove-margin-padding {
        margin: 0 !important;
        padding: 0 !important;
        width: 100% !important;
    }

    #menu {
        margin-bottom: 20px;
        padding-bottom: 0px;
        min-height: 100vh;
        overflow: auto !important;
        height: 100%;
    }

    #menu {
        ::-webkit-scrollbar {
            width: 0em !important;  
        }
    }

    #content {
        height: auto !important;
        padding-bottom: 100px !important;
    }

    .slimScrollDiv {
        overflow: auto !important; 
       
    }

    .menu-mambo-position .slimScrollDiv, .menu-mambo-position .slimScrollDiv .slim-scroll  {
        height: auto !important;
    }

    #fake-data-menu {
        display: none;
    }

    #loaderOverlay {
        display: none;
        position: fixed;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.5);
        z-index: 9999;
        justify-content: center;
        align-items: center;
    }

    .loaderContainer {
        padding: 20px;
        background: white;
        border-radius: 5px;
        box-shadow: 0 0 10px rgba(0,0,0,0.5);
        text-align: center;
    }

    .spinner {
        border: 4px solid rgba(0,0,0,.1);
        border-radius: 50%;
        border-top: 4px solid #e04545;
        width: 40px;
        height: 40px;
        -webkit-animation: spin 1s linear infinite;
        animation: spin 1s linear infinite;
        margin: auto;
    }

    .loaderContainer p {
        margin-top: 12px;
        font-size: 18px;
        color: #000;
    }

    .slim-scroll {
    width: auto;
    padding-bottom: 85px;
    height: auto !important;
    }

    @-webkit-keyframes spin {
        0% { -webkit-transform: rotate(0deg); }
        100% { -webkit-transform: rotate(360deg); }
    }

    @keyframes spin {
        0% { transform: rotate(0deg); }
        100% { transform: rotate(360deg); }
    }

    @media screen and (max-width:767px) {
    #menu::-webkit-scrollbar {
        width: 0em !important;
    }

    #menu ul {
            height: 140%;
            overflow: auto;
        }
    }

    .menu-mambo-position .slimScrollDiv, .menu-mambo-position .slimScrollDiv .slim-scroll  {
        height: 100% !important;
    }

    .slim-scroll {
        height: auto !important;
    }
    </style>

    <script>
    function showMenuMobile() {
                $("#menu").addClass('remove-margin-padding')
                $("#btn-open-menu").addClass('hidden')
                $("#btn-close-menu").removeClass('hidden')
    
            }
    function closeMenuMobile() {
                $("#btn-open-menu").removeClass('hidden')
                $("#btn-close-menu").addClass('hidden')
            }
</script>
</head>

{% block body %}
<body class="">
    <div id="notification"></div>

    {% block loader %}{% endblock %}
    {% set plan = app.session.get('plan') %}
    {% set isRegularDomain = app.session.get('isRegularDomain') %}
    {% set activeModules = app.session.get('modulesActive') %}
    {% set formFields = app.session.get('fields') %}

    <!-- Main Container Fluid -->
    <div class="container-fluid fluid menu-left">
     
        <div class="navbar main hidden-print">
            <!-- Brand -->
{#            {% if isRegularDomain is defined and isRegularDomain != '0'%}#}
{#            <a title="{{ wspot_version }}" href="#" class="appbrand pull-left"><span>{{ (whiteLabel.companyName|lower != 'wspot') ? whiteLabel.companyName : ''}}</span></a>#}
{#            {% endif %}#}
            <!-- Menu Toggle Button -->

            <ul class="topnav pull-left">
            
                <li class="account btn-mambo-position hidden">
                    <button  id="btn-open-menu" type="button" class="btn btn-navbar hide-menu" onclick="showMenuMobile()">
                       <span class="glyphicons show_lines"><i></i></span>
                    </button>
                    <button id="btn-close-menu" type="button" style="left: 5px;" class="btn btn-navbar hide-menu hidden" onclick="closeMenuMobile()">
                       <span class="glyphicons remove_2"><i></i></span>
                    </button>
                   
                </li>
                {% if app.user.spotManager %}
                    <li class="account">
                        <a href="{{ path('spots_manager_index') }}" class="glyphicons left_arrow" ><i></i> Gerenciador de Spots</a>
                    </li>
                    <li>
                        <div style="margin-left: 1em; padding-left: 1em; border-left: 2px solid #fff;">
                            Você esta no Spot: <span style="font-weight: bolder;">{{ app.session.get("wspotClient").domain }}</span>
                            <span class="tooltip-wrapper">
                                <i class="icon-large icon-question-sign" tabindex="0" data-placement="down" role="button" data-toggle="popover" data-trigger="focus" data-html="true" data-content="{{ app.session.get("wspotClient").company }}"></i>
                            </span>
                        </div>
                    </li>
                {% endif %}
            </ul>


            <!-- // Menu Toggle Button END -->

            <!-- Top Menu Right -->
            <ul class="topnav pull-right">

                <!-- Dropdown -->
                <li class="dropdown visible-abc">
                    <a href="" data-toggle="dropdown" class="glyphicons shield"><i></i> Proteção de Dados <span class="caret"></span></a>
                    <ul class="dropdown-menu pull-right">

                        <li><a href="{{ path('data_controller') }}">Controlador de Dados</a></li>
                        <li><a href="{{ path('consent_manager') }}">Gestão de Consentimento</a></li>
                        {% if is_granted("ROLE_MANAGER") %}
                            <li><a href="{{ path('legal_base_manager') }}">Gestão das Bases Legais</a></li>
                        {% endif %}
                    </ul>
                </li>
                {%  if app.session.get('isRegularDomain') %}
                <!-- // Dropdown END -->
                <li class="account">
                    <a href="https://suporte.mambowifi.com/support/solutions" target="_blank" class="glyphicons lightbulb white" style="padding: 0 38px 0 10px;" ><span class="hidden-phone text">Base de conhecimento</span><i style="position:relative; float:right"></i></a>
                </li>
                <!-- Profile / Logout menu -->
                {% endif %}
                <li class="account">
                    <a data-toggle="dropdown" href="{{ path('admin_profile_edit') }}" class="glyphicons logout lock"><span class="hidden-phone text">{{ app.user.nome }}</span><i></i></a>
                    <ul class="dropdown-menu pull-right">
                        <li class="highlight profile">
                            <span>
                                <span class="heading">Perfil <a href="{{ path('admin_profile_edit') }}" class="pull-right">editar</a></span>
                                <span class="img"></span>
                                <span class="details">
                                    <a href="{{ path('admin_profile_edit') }}">{{ app.user.nome }}</a> <br/>
                                    {{ app.user.username }}
                                </span>
                                <span class="clearfix"></span>
                                {% if (not is_granted("ROLE_USER_LIMITED")) and (not is_granted("ROLE_MARKETING_LIMITED")) and (not is_granted("ROLE_USER_BASIC")) %}
                                    <div id="relatorios-auditoria" class="{% if app.request.attributes.get('_route') == 'admin_audit_report' %}active{% endif %}" {% if plan == false %}style="opacity: 0.5"{% endif %}>
                                        </br>
                                        <a href="{{ path('admin_audit_report') }}">
                                            <span>Auditoria Administrativa</span>
                                        </a>
                                    </div>
                                {% endif %}
                            </span>
                        </li>

{#                        Correção paliativa para remover acesso ao historico de alterações: WSPOTNEW-4869#}
{#                        <li>#}
{#                            <a class="logs" href="{{ path('logs_list') }}">Histórico de alterações</a>#}
{#                        </li>#}
                        <li>
                            <span>
                                <a class="btn btn-default btn-mini pull-right" href="{{ path('exit') }}">Sair</a>
                            </span>
                        </li>
                    </ul>
                </li>
            </ul>
            <!-- // Top Menu Right END -->
        </div>
        <!-- Top navbar END -->

        <!-- Sidebar menu & content wrapper -->
        <div id="wrapper">
            <!-- Sidebar Menu -->
            <div id="menu" class="hidden-phone hidden-print menu-mambo-position">
                <!-- Scrollable menu wrapper with Maximum height -->
                <div class="slim-scroll" >
                    <!-- Sidebar Profile -->
                    <span id="cf" class="profile" style="padding: 10px; ">
                        <img src="{{ whiteLabel.logotipo }}" />
                    </span>
                    <!-- // Sidebar Profile END -->

                    {% set active = '' %}

                    <!-- Regular Size Menu -->
                    <ul>
                        <li id="dashboard" class="glyphicons dashboard 
                            {% if app.request.attributes.get('_route') == 'admin_dashboard' %} active {% endif %}">
                            <a href="{{ path('admin_dashboard') }}">
                                <i></i><span>Dashboard</span>
                            </a>
                        </li>

                        {% set guest_active = '' %}
                        {% set list_guest_active = '' %}
                        {% set group_guest_ctive = '' %}

                        {% if app.request.attributes.get('_route') == 'admin_visitantes' or
                        app.request.attributes.get('_route') == 'admin_visitantes_show' or
                        app.request.attributes.get('_route') == 'admin_visitantes_create' or
                        app.request.attributes.get('_route') == 'admin_visitantes_edit' or
                        app.request.attributes.get('_route') == 'admin_visitantes_detail' or
                        app.request.attributes.get('_route') == 'admin_visitantes_detail_print' or
                        app.request.attributes.get('_route') == 'admin_visitantes_print' or
                        app.request.attributes.get('_route') == 'admin_visitantes_delete' or
                        app.request.attributes.get('_route') == 'guest_password_reset' or
                        app.request.attributes.get('_route') == 'guest_resend_confirmation_url'
                        %}
                            {% set guest_active = 'active' %}
                            {% set list_guest_active = 'active' %}
                        {% endif %}

                        {% if app.request.attributes.get('_route') == 'group_list' or
                        app.request.attributes.get('_route') == 'group_create' or
                        app.request.attributes.get('_route') == 'group_edit' or
                        app.request.attributes.get('_route') == 'group_delete' or
                        app.request.attributes.get('_route') == 'group_show'
                        %}
                            {% set guest_active = 'active' %}
                            {% set group_guest_ctive = 'active' %}
                        {% endif %}
                        <li id="visitantes" class="hasSubmenu glyphicons group {{ guest_active }}">
                            <a href="#menu_guests" data-toggle="collapse" class=""><i></i><span>Visitantes</span></a>
                            <span class="count">&#8595;</span>
                            <ul id="menu_guests" class="collapse" style="height: auto;">
                                {% if not is_granted("ROLE_USER_BASIC") %}
                                    <li id="visitantes-regras-acesso" class="{{ group_guest_ctive }}" {% if plan == false %}style="opacity: 0.5"{% endif %}>
                                        <a href="{{ path('group_list') }}">
                                            <span>Regras de Acesso</span>
                                        </a>
                                    </li>
                                {% endif %}
                                {% if is_granted("ROLE_SUPER_ADMIN") or is_granted("ROLE_ADMIN") or is_granted("ROLE_ADMIN_LIMITED") or is_granted("ROLE_USER") or is_granted("ROLE_MARKETING") or is_granted("ROLE_MANAGER") or is_granted("ROLE_USER_BASIC") %}
                                <li id="visitantes-submenu" class="{{ list_guest_active }}">
                                    <a href="{{ path('admin_visitantes') }}">
                                        <span>Visitantes</span>
                                    </a>
                                </li>
                                {% endif %}
                            </ul>
                        </li>
                        {% if is_granted("ROLE_ADMIN") or is_granted("ROLE_MANAGER") %}
                            <li id="administradores" class="glyphicons user
                                {% if app.request.attributes.get('_route') == 'admin_usuarios' %} active{% endif %}"
                                {% if plan == false %}style="opacity: 0.5"{% endif %}>
                                <a href="{{ path('admin_usuarios') }}"><i></i>
                                    <span>Administradores</span>
                                </a>
                            </li>
                        {% endif %}

                        {% set active = '' %}
                        {% set apActive = '' %}
                        {% set apgActive = '' %}

                        {% if app.request.attributes.get('_route') == 'access_points' or
                        app.request.attributes.get('_route') == 'access_points_new' or
                        app.request.attributes.get('_route') == 'access_points_edit' %}
                            {% set active = 'active' %}
                            {% set apActive = 'active' %}
                        {% endif %}

                        {% if app.request.attributes.get('_route') == 'access_points_groups' or
                        app.request.attributes.get('_route') == 'configurations_edit' or
                        app.request.attributes.get('_route') == 'access_points_groups_new' or
                        app.request.attributes.get('_route') == 'access_points_groups_edit'%}
                            {% set active = 'active' %}
                            {% set apgActive = 'active' %}
                        {% endif %}

                        {% if is_granted("ROLE_ADMIN") or is_granted("ROLE_ADMIN_LIMITED") or is_granted("ROLE_USER") or is_granted("ROLE_MARKETING") or is_granted("ROLE_MANAGER") or is_granted("ROLE_USER_LIMITED") or is_granted("ROLE_MARKETING_LIMITED") or is_granted("ROLE_SUPORT_LIMITED") %}
                            <li id="ponto-acesso" class="hasSubmenu glyphicons router {{ active }}">
                                <a href="#menu_aps" data-toggle="collapse" class=""><i></i><span>Pontos de Acesso</span></a>
                                <span class="count">&#8595;</span>
                                <ul id="menu_aps" class="collapse" style="height: auto;">
                                    <li id="ponto-acesso-grupos" class="{{ apgActive }}" {% if plan == false %}style="opacity: 0.5"{% endif %}>
                                        <a href="{{ path('access_points_groups') }}">
                                            <span>Grupos</span>
                                        </a>
                                    </li>
                                    <li id="ponto-acesso-submenu" class="{{ apActive }}">
                                        <a href="{{ path('access_points') }}">
                                            <span>Pontos de Acesso</span>
                                        </a>
                                        {% if app.session.get('accessPointPendingName') > 0 %}
                                            <span class="count">{{ app.session.get('accessPointPendingName') }}</span>
                                        {% endif %}
                                    </li>
                                </ul>
                            </li>
                        {% endif %}

                        {% if is_granted("ROLE_ADMIN") or is_granted("ROLE_ADMIN_LIMITED") or is_granted("ROLE_MARKETING") or is_granted("ROLE_MANAGER") or is_granted("ROLE_MARKETING_LIMITED") or is_granted("ROLE_SUPORT_LIMITED") %}
                            <li id="templates" class="glyphicons pencil
                                {% if app.request.attributes.get('_route') == 'template' %} active{% endif %}"
                                {% if plan == false %}style="opacity: 0.5"{% endif %}>
                                <a href="{{ path('template') }}"><i></i>
                                    <span>Templates</span>
                                </a>
                            </li>
                        {% endif %}

                        {% if (is_granted("ROLE_ADMIN") or is_granted("ROLE_ADMIN_LIMITED") or is_granted("ROLE_MARKETING") or is_granted("ROLE_MANAGER") or is_granted("ROLE_MARKETING_LIMITED") or is_granted("ROLE_SUPORT_LIMITED")) and 'campaign' in activeModules %}
                            <li id="campanhas" class="glyphicons bullhorn
                                {% if app.request.attributes.get('_route') == 'campaign' %} active{% endif %}"
                                {% if plan == false %}style="opacity: 0.5"{% endif %}>
                                <a href="{{ path('campaign') }}"><i></i>
                                    <span>Campanhas</span>
                                </a>
                            </li>
                        {% endif %}

                        {% if (is_granted("ROLE_ADMIN") or is_granted("ROLE_ADMIN_LIMITED") or is_granted("ROLE_MARKETING") or is_granted("ROLE_MANAGER") or is_granted("ROLE_MARKETING_LIMITED") or is_granted("ROLE_SUPORT_LIMITED")) and 'sms_marketing' in activeModules%}
                            <li id="sms-marketing" class="glyphicons iphone
                                {% if app.request.attributes.get('_route') == 'sms_marketing' %} active{% endif %}"
                                {% if plan == false %}style="opacity: 0.5"{% endif %}>
                                <a href="{{ path('sms_marketing') }}"><i></i>
                                    <span>SMS Marketing</span>
                                </a>
                            </li>
                        {% endif %}

                        {% if (app.session.get('segmentationEnabled')) %}
                            {% if (is_granted("ROLE_ADMIN") or is_granted("ROLE_ADMIN_LIMITED") or is_granted("ROLE_MARKETING") or is_granted("ROLE_MANAGER") or is_granted("ROLE_MARKETING_LIMITED") or is_granted("ROLE_SUPORT_LIMITED")) and 'segmentation' in activeModules%}
                                <li id="segmentacao" class="glyphicons list
                                    {% if app.request.attributes.get('_route') == 'segmentation' %} active{% endif %}"
                                    {% if plan == false %}style="opacity: 0.5"{% endif %}>
                                    <a href="{{ path('segmentation') }}"><i></i>
                                        <span>Segmentação</span>
                                    </a>
                                </li>
                            {% endif %}
                        {% endif %}

                        {% if (is_granted("ROLE_ADMIN") or is_granted("ROLE_ADMIN_LIMITED") or is_granted("ROLE_MANAGER") or is_granted("ROLE_SUPORT_LIMITED")) and ('access_code' in activeModules) %}
                            <li id="codigo-acesso" class="glyphicons credit_card
                                {% if app.request.attributes.get('_route') == 'access_code' %} active{% endif %}"
                                {% if plan == false %}style="opacity: 0.5"{% endif %}>
                                <a href="{{ path('access_code') }}"><i></i>
                                    <span>Código de Acesso</span>
                                </a>
                            </li>
                        {% endif %}

