{
  "template":"radius_policy_*",
  "settings":{
    "index":{
      "number_of_shards":"5",
      "number_of_replicas":"1"
    }
  },
  "mappings":{
    "policy":{
      "_all":{
        "enabled":false
      },
      "properties":{
        "client":{
          "type":"nested",
          "properties":{
            "id":{
              "index":"not_analyzed",
              "type":"long"
            },
            "plan":{
              "index":"not_analyzed",
              "type":"string"
            },
            "apCheck":{
              "index":"not_analyzed",
              "type":"boolean"
            }
          }
        },
        "guest":{
          "type":"nested",
          "properties":{
            "employee":{
              "index":"not_analyzed",
              "type":"boolean"
            },
            "username":{
              "index":"not_analyzed",
              "type":"string"
            },
            "password":{
              "index":"not_analyzed",
              "type":"string"
            }
          }
        },
        "accessPoint":{
          "type":"nested",
          "properties":{
            "calledStationId":{
              "index":"not_analyzed",
              "type":"string"
            },
            "vendorName":{
              "index":"not_analyzed",
              "type":"string"
            },
            "calledStationName":{
              "index":"not_analyzed",
              "type":"string"
            },
            "callingStationId":{
              "index":"not_analyzed",
              "type":"string"
            },
            "routerMode":{
              "index":"not_analyzed",
              "type":"string"
            },
            "timezone":{
              "type":"string"
            }
          }
        },
        "timeLimit":{
          "type":"nested",
          "properties":{
            "hasTime":{
              "index":"not_analyzed",
              "type":"boolean"
            },
            "time":{
              "index":"not_analyzed",
              "type":"long"
            },
            "module":{
              "index":"not_analyzed",
              "type":"string"
            }
          }
        },
        "bandwidth":{
          "type":"nested",
          "properties":{
            "hasLimit":{
              "index":"not_analyzed",
              "type":"boolean"
            },
            "download":{
              "index":"not_analyzed",
              "type":"long"
            },
            "upload":{
              "index":"not_analyzed",
              "type":"long"
            }
          }
        },
        "created":{
          "format":"yyyy-MM-dd HH:mm:ss",
          "type":"date"
        }
      }
    }
  },
  "aliases":{
    "policy_current":{

    }
  }
}